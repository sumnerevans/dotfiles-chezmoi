text/html; elinks -dump %s; copiousoutput;

# PDF documents
application/pdf; zathura %s

# Images
image/jpg; feh %s
image/jpeg; feh %s
image/pjpeg; feh %s
image/png; feh %s
image/gif; feh %s

# iCal
text/calendar; /home/sumner/bin/icalview.py; copiousoutput
application/calendar; /home/sumner/bin/icalview.py; copiousoutput
application/ics; /home/sumner/bin/icalview.py; copiousoutput

# Office Suites
application/msword; libreoffice %s;
application/vnd.ms-word.document.12; libreoffice %s;
application/vnd.openxmlformats-officedocument.wordprocessingml.document; libreoffice %s;
application/vnd.oasis.opendocument.text; libreoffice %s;

# Microsoft LookOut
application/ms-tnef; tnef -w -C /home/sumner/tmp %s;
